{
  "name": "@ubits/tokens-ubits",
  "version": "1.0.0",
  "description": "Tokens UBITS oficiales como add-on intercambiable",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run build:tokens && tsc && vite build",
    "build:tokens": "node ../../tokens/build-css.cjs && cp ../../tokens/dist/tokens.css ./dist/tokens.css && cp ../../tokens/dist/tokens.js ./dist/tokens.js",
    "dev": "tsc --watch",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.3.0",
    "vite": "^7.1.12"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tokens.css": "./dist/tokens.css",
    "./tokens.js": "./dist/tokens.js"
  }
}

